{
    "name": "watson/bootstrap-form",
    "description": "Laravel 6 form wrappers for Bootstrap 4.",
    "keywords": ["laravel", "form", "bootstrap"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwightwatson@me.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/config": "~6.0 | ~7.0 | ~8.0",
        "illuminate/session": "~6.0 | ~7.0 | ~8.0",
        "illuminate/support": "~6.0 | ~7.0 | ~8.0",
        "laravelcollective/html": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Watson\\BootstrapForm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Watson\\BootstrapForm\\BootstrapFormServiceProvider"
            ],
            "aliases": {
                "BootForm": "Watson\\BootstrapForm\\Facades\\BootstrapForm"
            }
        }
    },
    "minimum-stability": "dev"
}
